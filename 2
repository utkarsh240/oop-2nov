#include <iostream>
#include <string>
using namespace std;

class Animal
{
public:
    Animal(string name, int age) : name(name), age(age) {}
    virtual void print() const
    {
        cout << "Name: " << name << endl;
        cout << "Age: " << age << endl;
    }
    virtual ~Animal() {}
private:
    string name;
    int age;

};

class Dog : public Animal
{
public:
    Dog(string name, int age, string breed) : Animal(name, age), breed(breed) {}
    void print() const
    {
        Animal::print();
        cout << "Breed: " << breed << endl;
    }
private:
    string breed;


};

int main()
{
    Animal *animal = new Dog("Rover", 5, "Labrador");
    animal->print();
    delete animal;
    return 0;
}
