#include<iostream>
using namespace std;

class Vehicle
{
    public:
        Vehicle()
        {
            cout<<"Vehicle constructor"<<endl;
        }
        ~Vehicle()
        {
            cout<<"Vehicle destructor"<<endl;
        }
};

class Car: public Vehicle
{
    public:
        Car()
        {
            cout<<"Car constructor"<<endl;
        }
        ~Car()
        {
            cout<<"Car destructor"<<endl;
        }
};

int main()
{
    Car c;
    Vehicle v = c;
    return 0;
}
